@using AntDesign.Icons;

<section>
    <div class="container mx-auto p-4">
        <h2 class="text-xl">
            NAM Repositories
        </h2>
        <hr class="rule my-4" />
        <div id="contribute_grid" class="grid grid-cols-1 md:grid-cols-2 gap-4">
            <div class="panel p-4 text-center">
                <img src="/svg/github-cat-logo/github-mark/github-mark-white.svg" alt="GitHub Logo"
                    class="w-24 mx-auto mb-6" />
                <h3 class="text-lg">neural-amp-modeler</h3>
                <hr class="rule my-4" />
                <p>
                    Neural network emulator for guitar amplifiers.
                </p>

                <br />
                <a class="btn btn-tertiary mb-4" target="_blank" href="https://github.com/sdatkinson/neural-amp-modeler">
                    <span>GitHub</span>
                    <ArrowRightOutlined />
                </a>
            </div>
            <div class="panel p-4 text-center">
                <img src="/svg/github-cat-logo/github-mark/github-mark-white.svg" alt="GitHub Logo"
                    class="w-24 mx-auto mb-6" />
                <h3 class="text-lg">NeuralAmpModelerCore</h3>
                <hr class="rule my-4" />
                <p>Core DSP library for NAM plugins</p>
                <br />
                <a class="btn btn-tertiary mb-4" target="_blank" href="https://github.com/sdatkinson/NeuralAmpModelerCore">
                    <span>GitHub</span>
                    <ArrowRightOutlined />
                </a>
            </div>
            <div class="panel p-4 text-center">
                <img src="/svg/github-cat-logo/github-mark/github-mark-white.svg" alt="GitHub Logo"
                    class="w-24 mx-auto mb-6" />
                <h3 class="text-lg">NeuralAmpModelerPlugin</h3>
                <hr class="rule my-4" />
                <p>Plugin for Neural Amp Modeler</p>
                <br />
                <a class="btn btn-tertiary mb-4" target="_blank" href="https://github.com/sdatkinson/NeuralAmpModelerPlugin">
                    <span>GitHub</span>
                    <ArrowRightOutlined />
                </a>
            </div>
            <div class="panel p-4 text-center">
                <img src="/svg/github-cat-logo/github-mark/github-mark-white.svg" alt="GitHub Logo"
                    class="w-24 mx-auto mb-6" />
                <h3 class="text-lg">NAMTrainerColab</h3>
                <hr class="rule my-4" />
                <p>NAM Trainer to be used on Google Colab</p>
                <br />
                <a class="btn btn-tertiary mb-4" target="_blank" href="https://github.com/sdatkinson/NAMTrainerColab">
                    <span>GitHub</span>
                    <ArrowRightOutlined />
                </a>
            </div>
        </div>
    </div>
</section>